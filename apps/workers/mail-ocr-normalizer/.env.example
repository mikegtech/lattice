# Kafka
KAFKA_BROKERS=localhost:29092
KAFKA_GROUP_ID=mail-ocr-normalizer
KAFKA_INPUT_TOPIC=lattice.ocr.result.v1
KAFKA_OUTPUT_TOPIC=lattice.mail.attachment.text.v1

# MinIO (for fetching OCR text)
MINIO_ENDPOINT=http://localhost:9000
S3_REGION=us-east-1
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin # pragma: allowlist secret
S3_FORCE_PATH_STYLE=true

# Worker
PORT=3000
SERVICE_NAME=mail-ocr-normalizer
LOG_LEVEL=info
